<ul class="breadcrumb">
	<li><%- linkTo('Administration Panel <span class="divider">/</span>', pathTo.admin()) %></li>
	<li><%- linkTo('User Management <span class="divider">/</span>', pathTo.admin_users()) %></li>
	<li><%- linkTo(user.displayName, pathTo.admin_users(user), { class: 'active' }) %></li>
</ul>

<div class="page-header">
	 <h1>Showing User | <%-user.displayName %></h1>
</div>
<table class="table table-bordered">
	<tbody>
		<tr>
			<th>id</th>
			<td><%-user.id %></td>
		</tr>
		<tr>
			<th>email</th>
			<td><%-user.email %></td>
		</tr>
		<tr>
			<th>displayName</th>
			<td><%-user.displayName %></td>
		</tr>
	</tbody>
</table>
<div class="well">
	<%- link_to( '<i class="icon-edit icon-white"></i> Edit', pathTo.edit_admin_user(user), {class: 'btn btn-primary'}) %>
	<%- link_to( '<i class="icon-remove icon-white"></i> Delete', pathTo.admin_user(user), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>or
	<%- link_to( 'Back to index', pathTo.admin_users()) %>
</div>